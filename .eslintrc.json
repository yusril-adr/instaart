{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "class-methods-use-this": "off",
        "consistent-return": "off",
        "linebreak-style": "off",
        "import/no-extraneous-dependencies": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "no-useless-return": "off",
        "radix": "off"
    }
}
